{
  "name": "tokexpress",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": " PORT=3001 node ./bin/www"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.css": "stylelint",
    "**/*.js": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{js,jsx,ts,tsx,md,html,css}": "prettier --write"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^8.2.0",
    "ejs": "^3.1.3",
    "express": "~4.17.3",
    "http-errors": "~1.6.3",
    "jade": "~1.11.0",
    "morgan": "~1.9.1",
    "nexmo": "^2.9.1",
    "opentok": "^2.10.0"
  },
  "devDependencies": {
    "eslint": "^6.6.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.20.6",
    "husky": "^4.2.5",
    "jsdoc": "^3.6.5",
    "nodemon": "^2.0.4",
    "prettier": "2.0.5",
    "prettier-eslint": "^11.0.0"
  }
}
